
<canvas class='canvas' style="width:{{qrcodeWidth}}px; height:{{qrcodeWidth}}px;" canvas-id='canvas' bindlongtap='save'></canvas>

<!-- image显示 -->
<view class='tip'>长按保存</view>

<view class="input-row">
  <label>网址</label>
  <input  bindinput='inputHandler' bindconfirm='confirmHandler' placeholder='输入需要转换为二维码的字符串' value='{{text}}'/>
</view>

<button bindtap='tapHandler' class="mybtn" type="primary">转为二维码</button>